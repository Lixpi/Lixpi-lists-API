{
    "name": "lists_api",
    "version": "0.0.1",
    "description": "Lists API",
    "main": "server.js",
    "scripts": {
        "test": "ENVIRONMENT=node_test node_modules/mocha/bin/_mocha ./src/**/*.test.js",
        "testLocal": "ENVIRONMENT=node_test_local node_modules/mocha/bin/_mocha ./src/**/*.test.js",
        "dev": "ENVIRONMENT=node_dev nodemon ./src/server.js",
        "devLocal": "ENVIRONMENT=node_dev_local nodemon ./src/server.js",
        "lint": "eslint ./src/ ./config",
        "devLint": "ENVIRONMENT=node_dev nodemon ./src/server.js --exec 'npm run lint && node'",
        "devLintLocal": "ENVIRONMENT=node_dev_local nodemon ./src/server.js --exec 'npm run lint && node'",
        "devInspectLocal": "ENVIRONMENT=node_dev_local nodemon --inspect=localhost:9229 ./src/server.js",
        "syncModels": "ENVIRONMENT=node_dev node -e 'require(\"./src/db/db-init\").syncModels()'",
        "syncModelsLocal": "ENVIRONMENT=node_dev_local node -e 'require(\"./src/db/db-init\").syncModels()'"
    },
    "dependencies": {
        "bcrypt": "*",
        "connect-session-sequelize": "*",
        "cors": "*",
        "dotenv": "*",
        "express": "*",
        "express-session": "*",
        "fsevents": "^2.1.2",
        "lodash": "*",
        "npm": "^6.14.2",
        "passport": "*",
        "passport-local": "*",
        "pg": "*",
        "pg-hstore": "*",
        "sequelize": "*",
        "sequelize-cli": "*",
        "uuid": "*"
    },
    "devDependencies": {
        "chai": "*",
        "debug": "*",
        "eslint": "*",
        "eslint-plugin-mocha": "*",
        "mocha": "*",
        "nodemon": "^2.0.2",
        "should": "*",
        "sinon": "*",
        "sinon-test": "*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Lixpi/Lixpi-lists-API.git"
    },
    "author": "Lixpi <lixpi@lixpi.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Lixpi/Lixpi-lists-API/issues"
    },
    "homepage": "https://github.com/Lixpi/Lixpi-lists-API#readme"
}
